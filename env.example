# AMS Frontend Environment Configuration
# Copy this file to .env.local and update the values

# API Configuration
VITE_API_URL=http://localhost:8002/api

# Be-Sphere URL for JWT validation
VITE_BE_SPHERE_URL=http://127.0.0.1:8000

# Sphere SSO login entry (redirect target will be /sso/callback on this app)
VITE_SPHERE_SSO_URL=http://127.0.0.1:8000/sso/login

# Application Configuration
VITE_APP_NAME=AMS
VITE_APP_VERSION=1.0.0

# Development Configuration
VITE_DEBUG=true
VITE_LOG_LEVEL=debug
